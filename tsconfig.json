{
	"compilerOptions": {
		"moduleResolution": "node",
		"outDir": "bin",
		"target": "ES2020",
		"rootDir": "./src/",
		"esModuleInterop": true,
		"module": "None",
		"jsx": "react",
		"allowUmdGlobalAccess": true,
		"strictBindCallApply": true
	},
	"compileOnSave": true,
	"exclude": [
		"node_modules"
	]
}